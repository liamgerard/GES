<UserControl x:Class="GES.DisabledKeysForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Width="300">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <ScrollViewer HorizontalScrollBarVisibility="Disabled">
        <Grid>
            <Border Padding="10" Background="#f8f8f8">
                <StackPanel Name="keysContent">
                    <!--<Grid Name="Grid1">
                        <TextBox Name="TB1" Style="{StaticResource KeyTextBox} " />
                        <Button Name="DB1" Content="X" Style="{StaticResource RemoveButtonStyle}" Margin="100,0,0,0"/>
                    </Grid>-->
                </StackPanel>
            </Border>
        </Grid>
    </ScrollViewer>
</UserControl>
